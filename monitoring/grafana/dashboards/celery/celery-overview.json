          "expr": "sum(rate(celery_task_revoked_total{worker=~\"$worker\", name=~\"$task\"}[5m]))",
          "legendFormat": "revoked"
        },
        {
          "refId": "G",
          "expr": "sum(rate(celery_task_rejected_total{worker=~\"$worker\", name=~\"$task\"}[5m]))",
          "legendFormat": "rejected"
